<!-- 服务价格 -->
<view class="baojie_price">
    <view class="table">
      <view class="tr">
       <view class="th">服务项目</view>
       <view class="th">服务价格</view>
      </view>
      <view class="tr" wx:for="{{date}}" wx:key="">
       <view class="td">{{item.p_name}}</view>
       <view class="td">{{item.p_price}}元/平米(10平米起)</view>
      </view>
    </view>
</view>
